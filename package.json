{
  "name": "text-committee-bot",
  "version": "0.0.0",
  "description": "HackSoc Committee Text Messaging Bot",
  "engines": {
    "node": ">= 10.13 <11"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "jest": "~23.6.0",
    "prettier": "1.15.3",
    "rimraf": "^2.6.3",
    "ts-jest": "~23.10.5",
    "tslint": "~5.12.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-microsoft-contrib": "~6.0.0",
    "tsutils": "~3.7.0",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "serve": "node build/index.js",
    "start": "npm run build && npm run serve",
    "dev": "./run-dev.sh",
    "export": "export $(grep -v '^#' .env | xargs)",
    "start-dev": "npm run export && npm run start"
  },
  "author": "Aaron Osher <aaron@aaronosher.io>",
  "license": "MIT",
  "dependencies": {
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.40",
    "@types/request-promise-native": "^1.0.15",
    "@types/sqlite3": "^3.1.3",
    "discord.js": "^11.4.2",
    "get-docker-secrets": "^0.1.7",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-router": "^7.4.0",
    "nexmo": "^2.4.0",
    "request-promise": "^4.2.4",
    "request-promise-native": "^1.0.7",
    "sqlite3": "^4.0.4",
    "tslib": "~1.9.3",
    "ukmsl-student-group-member-scraper": "^0.1.3"
  }
}
